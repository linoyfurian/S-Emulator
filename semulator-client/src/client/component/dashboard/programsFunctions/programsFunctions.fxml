<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="5.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.component.dashboard.programsFunctions.ProgramsFunctionsController">
   <children>
      <Label text="Available Programs">
         <padding>
            <Insets top="5.0" />
         </padding>
      </Label>
      <TableView fx:id="programsTbl" prefHeight="200.0">
        <columns>
          <TableColumn fx:id="programsTblNameCol" prefWidth="101.0" text="Name" />
          <TableColumn fx:id="programsTblUserNameCol" prefWidth="89.0" text="User Name" />
            <TableColumn fx:id="programsTblInstructionsCol" prefWidth="118.0" text="Instructions Number" />
            <TableColumn fx:id="programsTblMaxDegreeCol" prefWidth="75.0" text="Max Degree" />
            <TableColumn fx:id="programsTblRunsCol" prefWidth="89.0" text="Runs Number" />
            <TableColumn fx:id="programsTblAverageCol" prefWidth="101.0" text="Average Credits" />
        </columns>
      </TableView>
      <HBox>
         <children>
            <Button fx:id="executeProgramBtn" disable="true" mnemonicParsing="false" onAction="#onBtnExecuteProgramListener" text="Execute Program" />
         </children>
         <padding>
            <Insets left="5.0" />
         </padding>
      </HBox>
      <Label text="Available Functions" />
      <TableView fx:id="functionsTbl" prefHeight="200.0">
        <columns>
          <TableColumn fx:id="functionsTblNameCol" prefWidth="95.0" text="Name" />
          <TableColumn fx:id="functionsTblProgramNameCol" prefWidth="128.0" text="Program Name" />
            <TableColumn fx:id="functionsTblUserNameCol" prefWidth="97.0" text="User Name" />
            <TableColumn fx:id="functionsTblInstructionsCol" prefWidth="127.0" text="Instructions Number" />
            <TableColumn fx:id="functionsTblMaxDegreeCol" prefWidth="75.0" text="Max Degree" />
        </columns>
      </TableView>
      <HBox>
         <children>
            <Button fx:id="executeFunctionBtn" disable="true" mnemonicParsing="false" onAction="#onBtnExecuteFunctionListener" text="Execute Function" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" />
         </padding></HBox>
   </children>
</VBox>
